{
  "name": "ebay-emails-api",
  "version": "1.0.0",
  "description": "Automated transactional emails for customer follow up on Ebay",
  "author": "Make It Count <mike@makeitcount.cc>",
  "scripts": {
    "test": "vows --spec --isolate",
    "start": "node src/app.js | bunyan",
    "predeploy": "echo im about to deploy",
    "postdeploy": "echo ive deployed",
    "prepublish": "coffee --bare --compile --output lib/foo src/foo/*.coffee",
    "migrate": "node_modules/.bin/sequelize db:migrate --config config/databases.js",
    "migrate-undo": "node_modules/.bin/sequelize db:migrate:undo --config config/databases.js --models-path src/models/",
    "nodemon": "nodemon src/app.js | bunyan"
  },
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sidewalksalsa/ebay-emails.git"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "config": "^1.26.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "csurf": "^1.9.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "jsonwebtoken": "^7.4.1",
    "lodash.difference": "^4.5.0",
    "md5": "^2.2.1",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "morgan-json": "^1.1.0",
    "mysql": "^2.13.0",
    "object-path": "^0.11.4",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "shortid": "^2.2.8",
    "sparkpost": "^2.1.2",
    "swagger-node-express": "^2.1.3",
    "verror": "^1.10.0",
    "xss": "^0.3.3"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0"
  },
  "private": true,
  "license": "MIT"
}
